% COMMAND-LINE: --incremental
x, y: BOOLEAN;
ASSERT x OR y;
% EXPECT: sat
CHECKSAT;
PUSH;
  ASSERT NOT x;
% EXPECT: sat
  CHECKSAT;
  PUSH;
    ASSERT NOT y;
% EXPECT: unsat
    CHECKSAT;
  POP;
% EXPECT: sat
  CHECKSAT;
POP;
% EXPECT: sat
CHECKSAT;
PUSH 2;
ASSERT x;
ASSERT NOT x;
% EXPECT: unsat
CHECKSAT;
POP 2;
% EXPECT: sat
CHECKSAT;


